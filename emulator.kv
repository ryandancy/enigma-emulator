#:kivy 1.9.1

<EmulatorGui>:
  gui: gui
  id: gui
  
  plugboard: plugboard
  rotor0: rotor0
  rotor1: rotor1
  rotor2: rotor2
  reflector: reflector
  
  orientation: 'vertical'
  spacing: 20
  padding: [5, 10]
  
  Label:
    text: 'Enigma Emulator'
    size_hint: (1, 0.1)
  RelativeLayout:
    Rotor:
      size_hint: (None, None)
      pos_hint: {'x': 0.29, 'y': 0.2}
      id: rotor0
    Rotor:
      size_hint: (None, None)
      pos_hint: {'x': 0.53, 'y': 0.2}
      id: rotor1
    Rotor:
      size_hint: (None, None)
      pos_hint: {'x': 0.77, 'y': 0.2}
      id: rotor2
      reflector: reflector
    Reflector:
      id: reflector
    Plugboard:
      size_hint: (None, None)
      pos_hint: {'x': 0, 'y': 0}
      id: plugboard
    PlugboardToRotorsConnection:
      id: pb2r
  PlaintextLabel:
    id: plaintext
  CiphertextLabel:
    id: ciphertext
  BoxLayout:
    orientation: 'horizontal'
    size_hint: (1, 0.075)
    ResetButton:
      id: reset_btn
    SwapsInput:
      id: swaps_input
    RotorsInput:
      id: rotors_input
    ReflectorInput:
      id: reflector_input

<PlaintextLabel@Label>:
  size_hint: (1, 0.075)
  font_name: 'RobotoMono-Regular'
  text_size: self.size
  halign: 'left'
  text: 'plaintext: '

<CiphertextLabel@Label>:
  size_hint: (1, 0.075)
  font_name: 'RobotoMono-Regular'
  text_size: self.size
  halign: 'left'
  text: 'ciphertext: '

<ResetButton@Button>:
  font_name: 'RobotoMono-Regular'
  text: 'Reset'
  text_size: self.size
  halign: 'center'
  valign: 'middle'
  on_press: self.parent.parent.reset()

<SwapsInput@TextInput>:
  font_name: 'RobotoMono-Regular'
  hint_text: 'Swaps e.g. "(\'A\', \'B\'), (\'C\', \'D\')"'
  text_size: self.size
  halign: 'left'
  valign: 'middle'
  multiline: False
  on_text_validate: self.parent.parent.ids.plugboard.update_swaps(self.text)

<RotorsInput@TextInput>:
  font_name: 'RobotoMono-Regular'
  hint_text: 'Rotors'
  text: "('III', 'II', 'I')"
  halign: 'left'
  valign: 'middle'
  multiline: False
  on_text_validate: self.parent.parent.update_rotors(self.text)

<ReflectorInput@TextInput>:
  font_name: 'RobotoMono-Regular'
  hint_text: 'Reflector'
  text: 'B'
  halign: 'left'
  valign: 'middle'
  multiline: False
  on_text_validate: self.parent.parent.update_reflector(self.text)

