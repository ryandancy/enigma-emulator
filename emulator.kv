    canvas:
      Color:
        rgba: (0, 0, 0, 0) if self.parent.rotor_num == 2 else (0, 1, 0, 1) if self.parent.char_in and self.parent.cipher[self.parent.char_in] == 'A' else (1, 0, 0, 1) if self.parent.char_out and self.parent.cipher[self.parent.char_out] == 'A' else (0.3, 0.3, 0.3, 1)
