#:kivy 1.9.1

<EmulatorGui>:
  plugboard: plugboard
  rotor0: rotor0
  rotor1: rotor1
  rotor2: rotor2
  orientation: 'vertical'
  Label:
    text: 'Enigma Emulator'
  RelativeLayout:
    Rotor:
      size_hint: (None, None)
      pos_hint: {'x': 0.1, 'y': 0.4}
      id: rotor0
    Rotor:
      size_hint: (None, None)
      pos_hint: {'x': 0.4, 'y': 0.4}
      id: rotor1
    Rotor:
      size_hint: (None, None)
      pos_hint: {'x': 0.7, 'y': 0.4}
      id: rotor2
    Plugboard:
      size_hint: (None, None)
      id: plugboard

